unction themes_toPrimitive(t, r) { if (\"object\" != typeof t || !t) return t; var e = t[Symbol.toPrimitive]; if (void 0 !== e) { var i = e.call(t, r || \"default\"); if (\"object\" != typeof i) return i; throw new TypeError(\"@@toPrimitive must return a primitive value.\"); } return (\"string\" === r ? String : Number)(t); }\n\n\n\n\n\n\n\n\n\n\nconst ACCENT_PURPLE = 'purple';\nconst ACCENT_BLUE = 'blue';\nconst ACCENT_RED = 'red';\nconst ACCENT_RAINBOW = 'rainbow';\nconst ACCENT_MAP = {\n  [ACCENT_PURPLE]: purple_namespaceObject,\n  [ACCENT_BLUE]: blue_namespaceObject,\n  [ACCENT_RED]: red_namespaceObject,\n  [ACCENT_RAINBOW]: rainbow_namespaceObject\n};\nconst ACCENT_DEFAULT = ACCENT_RED;\nconst GUI_LIGHT = 'light';\nconst GUI_DARK = 'dark';\nconst GUI_MAP = {\n  [GUI_LIGHT]: light_namespaceObject,\n  [GUI_DARK]: dark_namespaceObject\n};\nconst GUI_DEFAULT = GUI_LIGHT;\nconst BLOCKS_THREE = 'three';\nconst BLOCKS_DARK = 'dark';\nconst BLOCKS_HIGH_CONTRAST = 'high-contrast';\nconst BLOCKS_CUSTOM = 'custom';\nconst BLOCKS_DEFAULT = BLOCKS_THREE;\nconst defaultBlockColors = three_blockColors;\nconst BLOCKS_MAP = {\n  [BLOCKS_THREE]: {\n    blocksMediaFolder: 'blocks-media/default',\n    colors: three_blockColors,\n    extensions: extensions,\n    customExtensionColors: {},\n    useForStage: true\n  },\n  [BLOCKS_HIGH_CONTRAST]: {\n    blocksMediaFolder: 'blocks-media/high-contrast',\n    colors: lodash_defaultsdeep_default()({}, high_contrast_blockColors, defaultBlockColors),\n    extensions: high_contrast_extensions,\n    customExtensionColors: customExtensionColors,\n    useForStage: true\n  },\n  [BLOCKS_DARK]: {\n    blocksMediaFolder: 'blocks-media/default',\n    colors: lodash_defaultsdeep_default()({}, blocks_dark_blockColors, defaultBlockColors),\n    extensions: dark_extensions,\n    customExtensionColors: dark_customExtensionColors,\n    useForStage: false\n  },\n  [BLOCKS_CUSTOM]: {\n    // to be filled by editor-theme3 addon\n    blocksMediaFolder: 'bloc