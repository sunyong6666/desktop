{
  "name": "react-intl-redux",
  "version": "0.7.0",
  "description": "Redux binding for React Intl",
  "main": "lib/index",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "clean": "del lib coverage",
    "lint": "eslint --ignore-path .gitignore --ext js,md .",
    "prepublish": "run-s clean build",
    "test": "run-p test:* lint",
    "test:ava": "ava",
    "coverage": "nyc ava",
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "react-intl": "^2.2.2",
    "react-redux": "^5.0.1"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.0",
    "del-cli": "^1.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-config-concise": "^0.14.0",
    "eslint-config-concise-ava": "^0.14.0",
    "eslint-config-concise-esnext": "^0.14.0",
    "eslint-config-concise-react": "^0.13.0",
    "husky": "^0.14.3",
    "immutable": "^3.8.2",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^4.3.0",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.0.0",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0"
  },
  "repository": "ratson/react-intl-redux",
  "license": "MIT",
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "reactjs",
    "react-intl",
    "react-redux",
    "intl",
    "i18n",
    "redux"
  ],
  "ava": {
    "require": [
      "babel-register",
      "raf/polyfill",
      "jsdom-global/register",
      "./test/helpers/enzyme-config"
    ],
    "babel": "inherit"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  }
}
